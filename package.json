{
    "name": "ng2-pica_angular16",
    "version": "1.3.2",
    "description": "Angular wrapper for pica to resize images.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/exenestecnico/ng2-pica.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-pica.js",
    "main": "./dist/bundles/ng2-pica.umd.js",
    "typings": "./dist/ng2-pica.d.ts",
    "scripts": {
        "build": "npm run transpile && npm run package && npm run minify",
        "minify": "uglifyjs dist/bundles/ng2-pica.umd.js --compress --mangle --comments --output dist/bundles/ng2-pica.umd.min.js",
        "package": "rollup -c",
        "transpile": "ngc"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "pica",
        "image",
        "resize"
    ],
    "dependencies": {
        "exifr": ">=7.1.3",
        "pica": ">=9.0.1"
    },
    "peerDependencies": {
        "@angular/core": ">=16.2.0",
        "@angular/common": ">=16.2.0",
        "@angular/platform-browser": ">=16.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@angular/common": ">=16.2.0",
        "@angular/compiler": ">=16.2.0",
        "@angular/compiler-cli": ">=16.2.0",
        "@angular/core": ">=16.2.0",
        "@angular/platform-browser": ">=16.2.0",
        "@angular/platform-browser-dynamic": ">=16.2.0",
        "@angular/router": ">=16.2.0",
        "@types/node": ">=20.4.10",
        "core-js": ">=3.32.0",
        "reflect-metadata": "^0.1.13",
        "rollup": "^0.41.4",
        "rxjs": "^7.8.1",
        "tslint": "^5.20.1",
        "typescript": "5.1.6",
        "uglify-js": "^3.17.4",
        "zone.js": "^0.13.1"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-pica/issues"
    },
    "homepage": "https://github.com/bergben/ng2-pica#readme"
}
